//*--------------------------------------------------------------------
//* THIS JOB WILL ASSEMBLE THE SOUTSYS SYSTEM
//*
//* 1. INSURE THAT THE REQUIRED CHANGES HAVE BEEN MADE TO THE
//*    ASSEMBLY MEMBERS PRIOR TO RUNNING THIS JOB
//* 2. VERIFY THE ASM.SYSLIB MACLIBS
//* 3. VERIFY THE LNKLIB PARM  LIBRARY
//* 4. VERIFY THE HLQ.OBJLIB REFRERENCES
//* 5. VERIFY THE LKED.SYSLIB  LIBRARYS
//*    (NEEDS LIBRARY WHERE "ISPLINK" EXISTS)
//* 6. VERIFY THE LAST TWO STEPS THAT LINKEDIT AN SVC
//* 7. SUBMIT THIS JOB
//*--------------------------------------------------------------------
//ASMSOUT  PROC I=,O=,LEPARM='MAP',LNKLIB='SYS2.LINKLIB'
//ASM      EXEC PGM=ASMA90,     <=== ASM H
//          PARM='TERM,XREF(SHORT),DECK,LIST'
//SYSTERM  DD   SYSOUT=*
//SYSPRINT DD   SYSOUT=*
//SYSLIB   DD   DISP=SHR,DSN=SYS3.SOUTSYS.MACLIB,
//             DCB=(LRECL=80,RECFM=FB,BLKSIZE=6320)
//         DD   DISP=SHR,DSN=SYS1.MACLIB
//         DD   DISP=SHR,DSN=SYS1.AMODGEN
//SYSUT1   DD   SPACE=(CYL,(2,3)),UNIT=VIO
//SYSUT2   DD   SPACE=(CYL,(2,3)),UNIT=VIO
//SYSUT3   DD   SPACE=(CYL,(2,3)),UNIT=VIO
//SYSLIN   DD   DUMMY
//SYSPUNCH DD   SPACE=(CYL,(2,3)),UNIT=VIO,DISP=(,PASS),DSN=&&OBJ,
//             DCB=(LRECL=80,RECFM=FB,BLKSIZE=3200)
//SYSIN    DD   DISP=SHR,DSN=SYS3.SOUTSYS.ASM(&I)
//*--------------------------------------------------------------------
//LKED     EXEC PGM=IEWL,PARM='LIST,&LEPARM'
//SYSPRINT DD   SYSOUT=*
//SYSUT1   DD   SPACE=(CYL,(2,3)),UNIT=VIO
//SYSLIB   DD   DISP=SHR,DSN=ISP.SISPLOAD  <=== WHERE ISPLINK EXISTS
//SYSLMOD  DD   DISP=SHR,DSN=&LNKLIB(&O)
//SYSLIN   DD   DISP=(OLD,DELETE),DSN=&&OBJ
//         DD   DDNAME=SYSIN
//         PEND
//*--------------------------------------------------------------------
//*        ASSEMBLE AND LINK THE SERVICE ROUTINES TO LINKLIST
//*--------------------------------------------------------------------
//CMNFS00  EXEC ASMSOUT,I=CMNFS00@,O=CMNFS00
//SOUTCALL EXEC ASMSOUT,I=SOUTCALL,O=SOUTCALL,LEPARM='RENT,AC=1'
//SOUTCLN  EXEC ASMSOUT,I=SOUTCLN,O=SOUTCLN
//SOUTCPR  EXEC ASMSOUT,I=SOUTCPR,O=SOUTCPR
//SOUTDATE EXEC ASMSOUT,I=SOUTDATE,O=SOUTDATE
//SOUTPRT  EXEC ASMSOUT,I=SOUTPRT,O=SOUTPRT,LEPARM='AC=1'
//SOUTSEP  EXEC ASMSOUT,I=SOUTSEP,O=SOUTSEP,LEPARM='RENT,AC=1'
//SOUTSUB  EXEC ASMSOUT,I=SOUTSUB,O=SOUTSUB,LEPARM='AC=1'
//SOUTWTR  EXEC ASMSOUT,I=SOUTWTR,O=SOUTWTR,LEPARM='AC=1'
//SOUTXIT  EXEC ASMSOUT,I=SOUTXIT,O=SOUTXIT
//LOCINDEX EXEC ASMSOUT,I=LOCINDEX,O=LOCINDEX
//*--------------------------------------------------------------------
//* ASM/LKED THE CONVERSION PROGRAM IF NEEDED
//*--------------------------------------------------------------------
//SOUTCVT EXEC ASMSOUT,I=SOUTCVT,O=SOUTCVT
//*--------------------------------------------------------------------
//* THIS JCL IS USED TO ASSEMBLE AND LINK THE SVC
//*--------------------------------------------------------------------
//ASMSVC   EXEC  PGM=ASMA90,
//            PARM='NODECK,OBJECT,XREF(SHORT),LIST'
//SYSUT1   DD DSN=&&SYSUT1,SPACE=(4096,(120,120),,,ROUND),
//            UNIT=SYSDA,DCB=BUFNO=1
//SYSLIN   DD DISP=SHR,DSN=SYS3.SOUTSYS.OBJLIB(IGC0025B)      <=======
//SYSLIB   DD DSN=SYS3.SOUTSYS.ASM,DISP=SHR,DCB=(BLKSIZE=9600)
//         DD DSN=SYS3.SOUTSYS.MACLIB,DISP=SHR
//         DD DISP=SHR,DSN=SYS1.SHASMAC
//         DD DISP=SHR,DSN=SYS1.MACLIB
//         DD DISP=SHR,DSN=SYS1.AMODGEN
//SYSPRINT DD SYSOUT=*
//SYSIN    DD DSN=SYS3.SOUTSYS.ASM(MVSGPSVC),DISP=SHR
//*--------------------------------------------------------------------
//LKEDSVC  EXEC PGM=IEWL,REGION=2000K,COND=(4,LT,ASMSVC),
//         PARM=(NCAL,LET,RENT,LIST,XREF)
//SYSPRINT DD SYSOUT=*
//SYSUT1   DD DSN=&&SYSUT1,UNIT=SYSDA,SPACE=(CYL,(20,20))
//OBJLIB   DD DISP=SHR,DSN=SYS3.SOUTSYS.OBJLIB          <==========
//SYSLMOD  DD DSN=SYS1.LPALIB,DISP=SHR
//SYSLIN   DD *
  INCLUDE OBJLIB(IGC0025B)
  NAME    IGC0025B(R)
